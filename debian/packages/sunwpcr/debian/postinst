PATH=/var/lib/dpkg/alien/sunwpcr:/bin:/usr/bin:/sbin:/usr/sbin; export PATH
ARCH=i386;				export ARCH
PKGINST=sunwpcr;			export PKGINST
PKG=sunwpcr;				export PKG
EXT=;					export EXT
SUN_PERSONALITY=1;				export SUN_PERSONALITY
BASEDIR=/etc/../;				export BASEDIR
PKG_INSTALL_ROOT=/etc/../;				export PKG_INSTALL_ROOT
INST_DATADIR=/var/lib/dpkg/alien;	export INST_DATADIR
UPDATE=
cat /var/lib/dpkg/status | grep "^Package:\s* sunwpcr" >/dev/null && UPDATE=yes
export UPDATE

alien_atexit() {
	true;
	echo Setting up class: preserve /etc/printers.conf
	echo $INST_DATADIR/$PKG/reloc/etc/printers.conf /etc/printers.conf | \
	    $INST_DATADIR/$PKG/i.preserve e preserve etc/printers.conf 0644 root sys
	chmod 0644 $BASEDIR/etc/printers.conf
	chown root $BASEDIR/etc/printers.conf
	chgrp sys $BASEDIR/etc/printers.conf
	echo Setting up class: manifest /var/svc/manifest/application/print/rfc1179.xml
	echo $INST_DATADIR/$PKG/reloc/var/svc/manifest/application/print/rfc1179.xml /var/svc/manifest/application/print/rfc1179.xml | \
	    $INST_DATADIR/$PKG/i.manifest f manifest var/svc/manifest/application/print/rfc1179.xml 0444 root sys
	chmod 0444 $BASEDIR/var/svc/manifest/application/print/rfc1179.xml
	chown root $BASEDIR/var/svc/manifest/application/print/rfc1179.xml
	chgrp sys $BASEDIR/var/svc/manifest/application/print/rfc1179.xml
}

trap alien_atexit EXIT

/var/lib/dpkg/alien/sunwpcr/postinstall
exit $?
