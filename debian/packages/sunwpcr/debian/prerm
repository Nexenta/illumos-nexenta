PATH=/var/lib/dpkg/alien/sunwpcr:/bin:/usr/bin:/sbin:/usr/sbin; export PATH
ARCH=i386;				export ARCH
PKGINST=sunwpcr;			export PKGINST
PKG=sunwpcr;				export PKG
EXT=;					export EXT
SUN_PERSONALITY=1;				export SUN_PERSONALITY
BASEDIR=/etc/../;				export BASEDIR
PKG_INSTALL_ROOT=/etc/../;				export PKG_INSTALL_ROOT
INST_DATADIR=/var/lib/dpkg/alien;	export INST_DATADIR
UPDATE=
cat /var/lib/dpkg/status | grep "^Package:\s* sunwpcr" >/dev/null && UPDATE=yes
export UPDATE

alien_atexit() {
	true;
	echo Cleaning up class: manifest /var/svc/manifest/application/print/rfc1179.xml
	echo /var/svc/manifest/application/print/rfc1179.xml | \
	    $INST_DATADIR/$PKG/r.manifest f manifest var/svc/manifest/application/print/rfc1179.xml 0444 root sys
}

test x$1 != xupgrade && alien_atexit; true

exit $?
