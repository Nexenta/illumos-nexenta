#ident  "@(#)dvorak 1.0     06/08/04"
#
# Dvorak key layout by Joshua Honeycutt
#
#Place in /usr/share/lib/keytables 
#Use loadkeys to switch keymaps
#

key 13   all shiftkeys+altgraph up shiftkeys+altgraph
key 15   all hole
key 19   all shiftkeys+alt up shiftkeys+alt
key 30   base 1 shift ! caps 1 ctrl 1 altg nop
key 31   base 2 shift @ caps 2 ctrl ^@ altg nop
key 32   base 3 shift # caps 3 ctrl 3 altg nop
key 33   base 4 shift $ caps 4 ctrl 4 altg nop
key 34   base 5 shift % caps 5 ctrl 5 altg nop
key 35   base 6 shift ^ caps 6 ctrl ^^ altg nop
key 36   base 7 shift & caps 7 ctrl 7 altg nop
key 37   base 8 shift * caps 8 ctrl 8 altg nop
key 38   base 9 shift ( caps 9 ctrl 9 altg nop
key 39   base 0 shift ) caps 0 ctrl 0 altg nop
key 40   base [ shift { caps [ ctrl ^[ altg nop
key 41   base ] shift } caps ] ctrl ^] altg nop
key 42   base ` shift ~ caps ` ctrl ^^ altg nop
key 54   base '\'' shift '"' caps '\'' ctrl '\'' altg nop
key 55   base , shift < caps , ctrl , altg nop
key 56   base . shift > caps . ctrl . altg nop
key 57   base p shift P caps P ctrl ^P altg nop
key 58   base y shift Y caps Y ctrl ^Y altg nop
key 59   base f shift F caps F ctrl ^F altg nop
key 60   base g shift G caps G ctrl ^G altg nop
key 61   base c shift C caps C ctrl ^C altg nop
key 62   base r shift R caps R ctrl ^R altg nop
key 63   base l shift L caps L ctrl ^L altg nop
key 64   base / shift ? caps / ctrl ^_ altg nop
key 65   base = shift + caps = ctrl = altg nop
key 67   all compose
key 76   all shiftkeys+leftctrl up shiftkeys+leftctrl
key 77   base a shift A caps A ctrl ^A altg nop
key 78   base o shift O caps O ctrl ^O altg nop
key 79   base e shift E caps E ctrl ^E altg nop
key 80   base u shift U caps U ctrl ^U altg nop
key 81   base i shift I caps I ctrl ^I altg nop
key 82   base d shift D caps D ctrl ^D altg nop
key 83   base h shift H caps H ctrl ^H altg nop
key 84   base t shift T caps T ctrl ^T altg nop
key 85   base n shift N caps N ctrl ^N altg nop
key 86   base s shift S caps S ctrl ^S altg nop
key 87   base - shift _ caps - ctrl ^_ altg nop
key 88   base '\\' shift | caps '\\' ctrl ^\ altg nop
key 100  base ; shift : caps ; ctrl ; altg nop
key 101  base q shift Q caps Q ctrl ^Q altg nop
key 102  base j shift J caps J ctrl ^J altg nop
key 103  base k shift K caps K ctrl ^K altg nop
key 104  base x shift X caps X ctrl ^X altg nop
key 105  base b shift B caps B ctrl ^B altg nop
key 106  base m shift M caps M ctrl '\r' altg nop
key 107  base w shift W caps W ctrl ^W altg nop
key 108  base v shift V caps V ctrl ^V altg nop
key 109  base z shift Z caps Z ctrl ^Z altg nop
key 119  all shiftkeys+capslock
key 124  all hole