# The log tag array, if you need to add other tag, nfslog.conf 
# should also be changed.
Log_opts="log			log=Sok_Lok		log=Sok_L_Bok
	log=Sok_L_B_FHok	log=Sok_Babst		log=Sok_FHabst
	log=Sok_L_Babst		log=Sok_Labst		log=Sok_DFT_RES
	log=Sok_DFT_ABST	log=Sok_DFT_DFLT 	log=Sok_FH_B_Labst
	log=Sok_Ldef_FH_Babst 	log=Sok_L_Bdef_FHabst	log=Sok_B_Lres_FHabst
	log=Sok_B_FHres_Labst	log=Sok_L_FHres_Babst"

# The NFS version options array for test combination generation
# Current nfslogd support is only NFSv3 and NFSv2
#
VEROPTS="vers=3 vers=2"

