<service_bundle type="manifest" name="testing_cd">

<service
	name="who_cares_service
	type="service"
	version="1">

	<instance
		name="who_cares_instance"
		enabled="false">

	<exec_method
		type="method"
		name="start"
		exec="%{cfg/cmd} -l %{cfg/cmd} -f %{cfg/state} -s %s -i %i -m %m"
		timeout_seconds="90" />
	<exec_method
		type="method"
		name="stop"
		exec="%{cfg/cmd} -l %{cfg/cmd} -f %{cfg/state} -s %s -i %i -m %m"
		timeout_seconds="90" />

	<property_group name="cfg" type="application">

		<property
			name="cmd"
			type="astring">
			<astring_list>
				<value_node
					value="/export/GL/proto/suites/smf/tests/svc.startd/bin/sparcv9/service" />
			</astring_list>
		</property>
                <property
			name="log"
			type="astring">
			<astring_list>
				<value_node
					value="/tmp/log.110485" /> 
			</astring_list>
		</property>

                <property
			name="state"
			type="astring">
			<astring_list>
				<value_node
					value="/tmp/state.110485" /> 
			</astring_list>
		</property>

	</property_group>
	</instance>

</service>
</service_bundle>
